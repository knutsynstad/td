{
  "private": true,
  "name": "td",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "npm run type-check && npm run lint:fix && npm run prettier && npm run test",
    "deploy": "npm run build && devvit upload",
    "dev": "devvit playtest",
    "dev:client": "vite build --watch",
    "dev:devvit": "devvit playtest",
    "dev:server": "vite build --watch",
    "dev:vite": "vite --port 7474",
    "icon:gen": "vite --config vite.icon.config.ts",
    "launch": "npm run build && npm run deploy && devvit publish",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "login": "devvit login",
    "prettier": "prettier-package-json --write ./package.json && prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "type-check": "tsc --build"
  },
  "dependencies": {
    "@devvit/start": "0.12.13",
    "@devvit/web": "0.12.13",
    "@hono/node-server": "^1.19.9",
    "devvit": "0.12.13",
    "hono": "^4.11.7",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@devvit/test": "0.12.13",
    "@eslint/js": "9.23.0",
    "@types/node": "^24.0.0",
    "@types/three": "^0.182.0",
    "concurrently": "9.1.2",
    "dotenv-cli": "8.0.0",
    "eslint": "9.23.0",
    "globals": "15.15.0",
    "jiti": "^2.6.1",
    "prettier": "3.5.3",
    "prettier-package-json": "2.8.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.54.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
